  },
  "files": [
    "README.md",
    "./dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gugaguichard/string-ts.git"
  },
  "bugs": {
    "url": "https://github.com/gugaguichard/string-ts/issues"
